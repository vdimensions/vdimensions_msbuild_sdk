<?xml version="1.0" encoding="utf-8"?>
<Project> 
  <!-- Assembly properties -->
  <PropertyGroup Condition=" '$(VersionPrefix)' != '' ">
    <AssemblyVersion Condition=" '$(AssemblyVersion)' == '' ">$(VersionPrefix)</AssemblyVersion>
    <AssemblyFileVersion Condition=" '$(AssemblyFileVersion)' == '' ">$(VersionPrefix)</AssemblyFileVersion>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Version)' != '' ">
    <AssemblyInformationalVersion Condition=" '$(AssemblyInformationalVersion)' == '' ">$(Version)</AssemblyInformationalVersion>
  </PropertyGroup>

  <!-- Package properties -->
  <PropertyGroup Condition=" '$(Version)' != '' ">
    <PackageVersion Condition=" '$(PackageVersion)' == '' ">$(Version)</PackageVersion>
  </PropertyGroup>
</Project>